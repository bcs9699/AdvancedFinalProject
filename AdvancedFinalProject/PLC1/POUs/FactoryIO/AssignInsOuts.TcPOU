FactoryIO_Outs [1].1 := FIO.b_OUT_9;
FactoryIO_Outs [1].2 := FIO.b_OUT_10;
FactoryIO_Outs [1].3 := FIO.b_OUT_11;
FactoryIO_Outs [1].4 := FIO.b_OUT_12;
FactoryIO_Outs [1].5 := FIO.b_OUT_13;

FactoryIO_RegOuts[0] := FIO.r_out_0;





]]></ST>
    </Implementation>
  </POU>
</TcPlcObject>