	sIPAddr:= '127.0.0.1', 
	nTCPPort:= 505, 
	nUnitID:= 1, 
	nQuantity:= 16, 
	nMBAddr:= 0, 
	cbLength:= SIZEOF(AssignInsOuts.FactoryIO_Outs), 
	pSrcAddr:= ADR(AssignInsOuts.FactoryIO_Outs), 
	bExecute:= TRUE, 
	tTimeout:= T#1S, 
	bBusy=> , 
	bError=> , 
	nErrId=> );
