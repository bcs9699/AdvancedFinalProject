	sIPAddr:= '127.0.0.1', 
	nTCPPort:= 505, 
	nUnitID:= 1, 
	nQuantity:= 8, 
	nMBAddr:= 0, 
	cbLength:=  SIZEOF(AssignInsOuts.FactoryIO_RegIns), 
	pDestAddr:= ADR(AssignInsOuts.FactoryIO_RegIns), 
	bExecute:= TRUE, 
	tTimeout:= T#1S, 
	bBusy=> , 
	bError=> , 
	nErrId=> , 
	cbRead=> );
